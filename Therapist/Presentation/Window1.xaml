<Window x:Class="Therapist.Presentation.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Therapist.Presentation"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="600">
    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Background="Aquamarine">
            <Button Width="125" Margin="15,0,0,0" BorderBrush="White"  Content="Слайд 1" Click="Button_Click"/>
            <Button Width="125" Margin="15,0,0,0" BorderBrush="White" Content="Слайд 2" Click="Button_Click_1"/>
            <Button Width="125" Margin="15,0,0,0" BorderBrush="White" Content="Слайд 3" Click="Button_Click_2"/>
            <Button Width="125" Margin="15,0,0,0" BorderBrush="White" Content="Слайд 4" Click="Button_Click_3"/>
        </StackPanel>
        <Frame Name="Main" Margin="0,35,0,0" Background="Aquamarine"/>
    </Grid>
</Window>
